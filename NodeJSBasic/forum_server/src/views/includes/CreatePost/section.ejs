<div class="create-content">
  <form method="post" enctype="multipart/form-data">
    <label for="title" class="form-label">제목</label>
    <div class="input-group mb-3 input-group-lg">
      <input type="text" id="title" class="form-control" name="title">
    </div>
    <label for="writer" class="form-label">작성자</label>
    <div class="input-group mb-3">
      <input type="text" id="writer" class="form-control" name="userName">
    </div>
    <label for="password" class="form-label">비밀번호</label>
    <div class="input-group mb-3">
      <input type="text" id="password" class="form-control" name="userPwd">
    </div>
    <label for="description" class="form-label">내용</label>
    <div class="input-group mb-3">
      <textarea id="description" class="form-control" name="description"></textarea>
    </div>
    <label class="form-label" for="thumbnail">썸네일</label>
    <div class="input-group mb-3">
      <input type="file" class="form-control" id="thumbnail" name="thumbnail" accept='image/jpg, image/png, image/jpeg' >
    </div>
    <div>
      <button type="submit" formaction='/create-post' class="btn btn-primary mb-3" >게시물 작성</button>
    </div>
  </form>
</div>